{
  "name": "knzklive-docs",
  "version": 2,
  "alias": ["knzklive-docs.knzk.me"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/(.+)\\.(.+)", "dest": "/$1.$2" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ],
  "regions": ["hnd1"],
  "github": { "silent": true }
}
